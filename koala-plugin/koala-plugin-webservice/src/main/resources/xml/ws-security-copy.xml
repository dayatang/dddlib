<?xml version="1.0" encoding="UTF-8"?>
<XmlParse>

    <!-- WS client生成的所在 -->
	<CopyFiles>
		<property name="srcDir">ws-security-template/war-ws</property>
		<property name="destDir">${path}/war-ws/</property>
	</CopyFiles>
    
    <VelocityFileObject>
      <property name="vm">vm/ws/war-ws/pom.xml.vm</property>
      <property name="path">${path}/war-ws/pom.xml</property>
    </VelocityFileObject>
	
	<VelocityFileObject>
      <property name="vm">vm/ws/war-ws/src/main/resources/META-INF/spring/root.xml.vm</property>
      <property name="path">${path}/war-ws/src/main/resources/META-INF/spring/root.xml</property>
    </VelocityFileObject>
    
    <VelocityFileObject>
      <property name="vm">vm/ws/war-ws/src/main/resources/META-INF/spring/spring-cxf.xml.vm</property>
      <property name="path">${path}/war-ws/src/main/resources/META-INF/spring/spring-cxf.xml</property>
    </VelocityFileObject>
    
    <VelocityFileObject>
      <property name="vm">vm/ws/war-ws/src/main/webapp/WEB-INF/web.xml.vm</property>
      <property name="path">${path}/war-ws/src/main/webapp/WEB-INF/web.xml</property>
    </VelocityFileObject>
    
</XmlParse>